<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/button"    tools:context=".ResultActivity">    <Button        android:id="@+id/button1"        android:layout_width="match_parent"        android:layout_height="72dp"        android:layout_marginTop="104dp"        android:background="@drawable/bg_bottom_buttomhmm"        android:drawableLeft="@drawable/images15"        android:drawableRight="@drawable/arry4"        android:text="1st To 3rd Semester Result"        android:textColor="#000"        android:textSize="18sp"        android:textStyle="normal|bold|italic"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <Button        android:id="@+id/button2"        android:layout_width="match_parent"        android:layout_height="72dp"        android:layout_marginTop="32dp"        android:background="@drawable/bg_bottom_buttomhmm"        android:drawableLeft="@drawable/images15"        android:drawableRight="@drawable/arry4"        android:text="4th TO 8th Semester Result"        android:textColor="#000"        android:textSize="18sp"        android:textStyle="normal|bold|italic"        app:layout_constraintBottom_toTopOf="@+id/button3"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.52"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@+id/button1" />    <Button        android:id="@+id/button16"        android:layout_width="296dp"        android:layout_height="46dp"        android:layout_marginTop="8dp"        android:background="@drawable/bg_bottom_buttomhmm"        android:drawableLeft="@drawable/images15"        android:text="Result"        android:textColor="#000"        android:textSize="30sp"        android:textStyle="normal|bold|italic"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" /></androidx.constraintlayout.widget.ConstraintLayout>